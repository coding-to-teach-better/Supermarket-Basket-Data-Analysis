    "        WHEN Price > 1.50 AND Price < 3 THEN 'Mid-range'\n",
    "        ELSE 'Expensive'\n",
    "    END AS PriceCatagory\n",
    "FROM priceTable\n",
    "INNER JOIN volumeTable\n",
